<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.teamproject.mapper.ProjectMapper">


	<insert id="insertNormalMember"> 
		<!-- 통합회원 정보  -->
		insert into member(user_no)
		values(#{user_no})
	</insert>
	
	<!-- 간편회원 로그인 -->
	<insert id="insertSimpleMember">
		insert into member(user_no, access_Token)
		values(#{user_no}, #{access_Token})

	</insert>
	
	
	
	<!-- select 일때 리턴값 꼭 지정해주기!! -->
	<select id="userCheck" resultType="com.teamproject.domain.MemberDTO">
		select * from member
		where id= #{id} and pass= #{pass}
	
	</select>
	
	


</mapper>